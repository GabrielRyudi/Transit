<% if(typeof errors != 'undefined') { %>
    <% errors.forEach(function(error){ %>
        <p class="error"><%= error.msg %></p>
    <% }); %>
<% } %>

<% if(success_msg != ''){ %>
    <p class="error" style="color: green;"><%= success_msg %></p>
<% } %>

<% if(error_msg != ''){ %>
    <p class="error"><%= error_msg %></p>
<% } %>

<% if(error != ''){ %>
    <p class="error"><%= error %></p>
<% } %>